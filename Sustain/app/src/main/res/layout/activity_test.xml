<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.project.sustain.controllers.TestActivity">

    <Button
        android:id="@+id/btnTestGetAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test Get Address"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp"/>

    <Button
        android:id="@+id/btnTestGetLatLong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignWithParentIfMissing="false"
        android:text="Test Get Lat Long"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="80dp"
        android:layout_marginTop="7dp"
        android:layout_marginStart="22dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btnTestGetAddress"/>

</RelativeLayout>
